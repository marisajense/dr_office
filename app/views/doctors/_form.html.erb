<%= form_for doctor do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name, required: true %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name, required: true %>
  <%= f.label :speciality %>
  <%= f.text_field :speciality, required: true %>
  <%= f.submit nil, class: 'btn black' %>
<% end %>
